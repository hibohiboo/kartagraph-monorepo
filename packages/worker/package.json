{
  "name": "@kartagraph/worker",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf .turbo",
    "test": "jest --config ./jest.config.json",
    "watch": "jest --config ./jest.config.json --watch",
    "mv-jest-reports": "cp ./public/mockServiceWorker.js ../../docs/astro/public/ && mv ./__reports__ ../../docs/astro/public/jest-reports-components",
    "ncu": "ncu -u"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "vite": "^5.0.6"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "files": [
    "src"
  ]
}
