    "npm-check-updates": "^16.14.20",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
