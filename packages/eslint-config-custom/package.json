{
  "name": "@kartagraph/eslint-config-custom",
  "type": "module",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unused-imports": "^3.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  },
  "scripts": {
    "inspect": "npx @eslint/config-inspector",
    "build": "npx @eslint/config-inspector build && sed -i \"s|<head><meta|<head><base href='/public/eslint-config-inspector/'><meta|g\" .eslint-config-inspector/index.html && rm -rf ../../dist/public/eslint-config-inspector && cp -r .eslint-config-inspector ../../dist/public/eslint-config-inspector",
    "ncu": "ncu -u"
  }
}
