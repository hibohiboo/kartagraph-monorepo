{
  "extends": "@kartagraph/tsconfig/react",
  "include": ["src", "tests","./jest"],
  "compilerOptions": {
    "paths": {
      "@kartagraph-editor/*": ["./src/*"],
      "@kartagraph-ui/*": ["../packages/ui/src/*"],
      "@kartagraph-worker/*": ["../packages/worker/src/*"],
      "@kartagraph-types/*": ["../types/src/*"],
    },
    "esModuleInterop": true,
  },
  "references": [{ "path": "./tsconfig.node.json" }]
}