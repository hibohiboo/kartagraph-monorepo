{
  "name": "@kartagraph/editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "ncu": "ncu -u",
    "postncu": "msw init",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest -c jest.config.json",
    "watch": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest -c jest.config.json --watch"
  },
  "dependencies": {
    "firebase": "^10.14.1",
    "jotai": "^2.10.1",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.27.0",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@kartagraph/editor-ui": "*",
    "@kartagraph/eslint-config-custom": "*",
    "@kartagraph/tsconfig": "*",
    "@kartagraph/ui": "*",
    "@kartagraph/worker": "*",
    "@testing-library/jest-dom": "^6.5.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "msw": "^2.4.11",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "vite": "^5.4.9"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
