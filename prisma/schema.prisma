generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model scenario_list {
  id      String   @id @db.Uuid
  title   String
  src     String
  summary String
  detail  String
  s3_key  String
  created DateTime @default(now()) @db.Timestamptz(6)
  updated DateTime @default(now()) @updatedAt @db.Timestamptz(6)
}
