{
  "name": "kartagraph-api",
  "version": "0.0.1",
  "dependencies": {
    "@apiture/openapi-down-convert": "^0.11.0",
    "@redocly/cli": "1.9.0"
  },
  "private": true,
  "scripts": {
    "dev": "redocly preview-docs",
    "test": "redocly lint",
    "bundle": "redocly bundle -o dist/bundle.yaml",
    "postbundle": "openapi-down-convert -i dist/bundle.yaml -o dist/bundle-3.0.yaml",
    "build": "redocly build-docs core@v1 --disableGoogleFont -o ../../dist/public/kartagraph-openapi/index.html"
  }
}
